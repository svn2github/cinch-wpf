<Application x:Class="MVVM.Demo.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    StartupUri="Views/MainWindow.xaml">
    <Application.Resources>


        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Brushes.xaml"/>
                <ResourceDictionary Source="Resources/AppStyles.xaml"/>
                <ResourceDictionary Source="Resources/ListViewStyles.xaml"/>
                <ResourceDictionary Source="Resources/GlobalConverters.xaml"/>
                <ResourceDictionary Source="Resources/TabStyles.xaml"/>
                <ResourceDictionary Source="Resources/MenuStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>

    </Application.Resources>
</Application>
